/* pages/sb/list/list.wxss */


.add_class {
  /* background-color: #fff;  */
  width: 90%;
  height: 80rpx;
  color: #017ad7;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  font-size: 16px;
  border-radius: 50rpx;
  border: 1px solid #017ad7;
}

.scrolldb {
  height: calc(100% - 350rpx);
}

.scroll-view-item {
  border-bottom: 2rpx solid #eee;
  margin: 0 36rpx;
  padding: 30rpx 0;
}

.item1 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  flex-direction: row;
}

.dateLabel1 {
  color: #474747;
  width: 50%;
  word-break: break-all;
  word-wrap: break-word;
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

.dateLabel2 {
  color: #dbdbdb;
  text-align: right;
}

.meet {
  padding: 10rpx 30rpx;
  display: flex;
}

.meet .flex_1 {
  flex:2
}

page {
  background: #f5f5f5;
}

.head {
  text-align: center;
  font-weight: bold;
  font-size: 20px;
}

.table {
  margin-left: 2px;
  display: inline-flex;
  flex-direction: column;
  border: 1px solid rgba(218, 217, 217, 1);
